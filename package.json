{
  "name": "bds-hub",
  "version": "0.12.0",
  "description": "",
  "main": "index.js",
  "author": "Stephen O'Connor",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "archiver": "^5.3.1",
    "discord.js": "^14.11.0",
    "json-stringify-pretty-compact": "^4.0.0",
    "jsonpath-plus": "^7.2.0",
    "package.json": "^2.0.1",
    "rollup": "^3.27.0",
    "sqlite": "^5.0.1",
    "sqlite3": "^5.1.6",
    "typia": "^5.3.5",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@minecraft/server": "^1.9.0-beta.1.20.60-preview.25",
    "@minecraft/server-net": "^1.0.0-beta.1.20.30-stable",
    "@minecraft/server-ui": "^1.2.0-beta.1.20.30-stable",
    "@types/archiver": "^5.3.2",
    "@types/node": "^20.9.1",
    "adm-zip": "^0.5.10",
    "ava": "^5.3.1",
    "gts": "^5.2.0",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.1",
    "typescript": "^5.3.2"
  },
  "bin": {
    "hubStart": "dist/scripts/hubStart.js",
    "hubConfig": "dist/scripts/hubConfig.js",
    "hubPack": "dist/scripts/hubPack.js"
  },
  "scripts": {
    "build": "npm run types && tsc && npm run finish && npm run hubValidateExamples",
    "qbuild": "tsc && npm run finish",
    "prepare": "ts-patch install && typia patch && npm run compile",
    "test": "tsc && npx ava",
    "finish": "node dist/scripts/finishBuild.js",
    "rollup": "node dist/scripts/rollup.js",
    "types": "npx typia generate --input src/types/src --output src/types/gen --project tsconfig.json",
    "hubStart": "node dist/scripts/hubStart.js",
    "hubConfig": "node dist/scripts/hubConfig.js",
    "hubPack": "node dist/scripts/hubPack.js",
    "hubSupplemental": "node dist/scripts/hubSupplemental.js",
    "hubValidateExamples": "node dist/scripts/hubValidateExamples.js",
    "buildAndPack": "npm run build; npm run hubPack",
    "buildAndPackDev": "npm run build; npm run hubPack -- --preserveTempFiles",
    "buildAndInstall": "npm run build; npm run hubInstall",
    "buildAndInstallDev": "npm run build; npm run hubInstall -- --preserveTempFiles",
    "patch": "npm run build && npm version patch && npm publish",
    "minor": "npm run build && npm version minor && npm publish",
    "major": "npm run build && npm version major && npm publish",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run lint"
  }
}
